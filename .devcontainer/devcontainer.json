{
    "name": "Java Gradle",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "features": {
        "ghcr.io/devcontainers/features/java:1": {
        "version": "none",
        "installMaven": "false",
        "installGradle": "true"
        },
        "ghcr.io/devcontainers-contrib/features/ant-sdkman:2": {}
    },
    "customizations": {
        "vscode": {
            "settings": {
                "workbench.colorTheme": "Default Dark Modern"
            },
            "extensions": [
                "MCEngine.mcengine-vscode-extension-pack"
            ]
        }
    }
}
